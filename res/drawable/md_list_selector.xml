<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<solid android:color="@color/default_pressed" />
		</shape>
	</item>
	<item android:state_focused="true">
		<shape>
			<solid android:color="@color/default_pressed" />
		</shape>
	</item>
	<item android:state_selected="true" android:drawable="@color/md_list_selected" />
	<item android:drawable="@drawable/md_normal" />
</selector>
